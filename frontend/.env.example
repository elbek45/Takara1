# Takara Gold Frontend - Environment Configuration Example
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=https://api.takarafi.com/api
VITE_WS_URL=wss://api.takarafi.com

# Solana Network
# Use 'mainnet-beta' for production, 'devnet' or 'testnet' for development
VITE_SOLANA_NETWORK=mainnet-beta
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Application
VITE_APP_NAME=Takara Gold
VITE_APP_VERSION=2.1.1

# Feature Flags (optional)
VITE_ENABLE_DEBUG=false
